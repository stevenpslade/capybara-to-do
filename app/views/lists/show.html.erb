<%= link_to 'Add Task', new_list_task_path(@list) %>

<h2><%= @list.title%></h2>
<%= link_to 'Edit List', edit_list_path(@list) %>
<%= link_to 'Delete List', list_path(@list), method: :delete %>
<ul>
  <% @tasks.each do |task| %>
    <li><%= task.description %></li>
    <%= link_to 'Edit Task', edit_polymorphic_path([@list, task]) %>
    <%= link_to 'Delete Task', [@list, task], method: :delete %>
  <% end %>
</ul>